    "colors": "^1.1.0",
    "cssmin": "^0.4.3",
    "detachkify": "0.0.5",
    "es6ify": "^1.6.0",
    "event-stream": "^3.3.1",
    "glob": "^5.0.10",
    "handsontable": "git://github.com/handsontable/handsontable",
    "inherits": "^2.0.1",
    "inquirer": "^0.8.5",
    "minimist": "^1.1.1",
    "moment": "^2.9.0",
    "ncp": "^2.0.0",
    "npm": "^2.7.6",
    "pikaday": "^1.3.2",
